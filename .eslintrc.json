{
  "extends": [
    "next/core-web-vitals",
    "eslint:recommended",
    "airbnb-base",
    "airbnb-typescript"
  ],
  "env": {
    "jest": true
  },
  "overrides": [
    {
      "files": ["*.js", "*.ts"],
      "rules": {
        "import/extensions": ["error", "never"]
      }
    }
  ],
  "parserOptions": {
   "project": "./tsconfig.json"
  }
}
